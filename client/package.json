{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "concurrently": "^7.0.0",
    "ganache": "^7.0.2",
    "truffle": "^5.4.32",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
